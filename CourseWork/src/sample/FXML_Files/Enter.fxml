<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" style="-fx-background-color: ffffff;" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.EnterController">
   <children>
      <AnchorPane fx:id="mainPane" prefHeight="255.0" prefWidth="300.0">
         <children>
            <Label layoutX="33.0" layoutY="46.0" prefHeight="26.0" prefWidth="49.0" text="Логин">
               <font>
                  <Font name="Calibri" size="18.0" />
               </font>
            </Label>
            <Label layoutX="29.0" layoutY="128.0" text="Пароль">
               <font>
                  <Font name="Calibri" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textField1" layoutX="109.0" layoutY="46.0" onKeyPressed="#handleKeyPressed" prefHeight="25.0" prefWidth="164.0" />
            <Button fx:id="button1" layoutX="91.0" layoutY="180.0" mnemonicParsing="false" onAction="#enterButtonAction" text="Вход" />
            <Hyperlink fx:id="regLink" layoutX="90.0" layoutY="217.0" onAction="#registration" text="Зарегистрироваться" />
            <Button layoutX="151.0" layoutY="180.0" mnemonicParsing="false" onAction="#clearButtonAction" text="Отмена" />
            <PasswordField fx:id="textField2" layoutX="109.0" layoutY="127.0" onKeyPressed="#handleKeyPressed" prefHeight="25.0" prefWidth="164.0" />
         </children>
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </GridPane.margin>
      </AnchorPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
